{
    "manifestVersion": 1,
    "id": "vsts-open-work-items-in-excel",
    "version": "0.1.0",
    "name": "VSTS Open in Excel",
    "scopes": [ "vso.work", "vso.work_write" ],
    "description": "This extension opens work item query result in Excel (from Web)",
    "publisher": "vanvanvania",
    "icons": {
        "default": "img/logo.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "Sample"
    ],
    "content": {
        "details": {
            "path": "details.md"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://bit.ly"
        },
        "learn": {
            "uri": "https://bit.ly"
        },
        "support": {
            "uri": "https://bit.ly"
        }
    },
    "branding": {
        "color": "rgb(255, 255, 255)",
        "theme": "light"
    },
    "files": [
        {
            "path": "img",
            "addressable": true
        },
        {
            "path": "scripts",
            "addressable": true
        },
        {
            "path": "action.html",
            "addressable": true
        }
    ],
    "categories": [
        "Integrate"
    ],
    "contributions": [
        {
            "id": "openQueryAction",
            "type": "ms.vss-work-web.action",
            "description": "Open in Excel",
            "targets": [
                "ms.vss-work-web.work-item-query-menu"
            ],
            "properties": {
            	"text": "Open in Excel",
            	"title": "Requires Visual Studio DEV15",
                "uri": "action.html"
            }
        },
        {
            "id": "openWorkItemsAction",
            "type": "ms.vss-work-web.action-provider",
            "description": "Open in Excel",
            "targets": [
                "ms.vss-work-web.work-item-context-menu"
            ],
            "properties": {
                "text": "Open in Excel",
                "title": "Open in Excel",
                "uri": "action.html"
            }
        },
        {
            "id": "openQueryOnToolbarAction",
            "type": "ms.vss-work-web.action",
            "description": "Open in Excel",
            "targets": [
                "ms.vss-work-web.work-item-query-toolbar-menu"
            ],
            "properties": {
                "text": "Open in Excel",
                "title": "Open in Excel",
                "uri": "action.html"
            }
        }
    ]
}
